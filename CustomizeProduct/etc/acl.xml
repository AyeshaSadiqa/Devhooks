<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
   <acl>
      <resources>
         <resource id="Magento_Backend::admin">
            <!-- this resource id we can use in system.xml for section -->
            <resource id="FME_CustomizeProduct::grid" title="Customize Product" translate="title" sortOrder="10">
               <resource id="FME_CustomizeProduct::upload" title="upload Image" translate="title" sortOrder="10" />
               <resource id="FME_CustomizeProduct::save" title="Save Data changes" translate="title" sortOrder="20" />
               <resource id="FME_CustomizeProduct::enable" title="Enable Images" translate="title" sortOrder="30" />
               <resource id="FME_CustomizeProduct::disable" title="Disable Images" translate="title" sortOrder="40" />
               <resource id="FME_CustomizeProduct::delete" title="Delete Grid Rows" translate="title" sortOrder="50" />
               <resource id="FME_CustomizeProduct::edit" title="Edit Grid / Create Form" translate="title" sortOrder="60" />
            </resource>
         </resource>
      </resources>
   </acl>
</config>